ec-canvas {
  width: 100%;
  height: 100%;
}
.container{
  height: 700rpx;
  padding-top: 30rpx;
}
.tab-item{
  width: 250rpx;
  display: inline-block;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
}
.tab-container{
  border-bottom: 1rpx solid #05C9D3;
  display: fixed;
  top: 0;
  left: 0;
}
.tab-select{
  background-color: #05C9D3;
  color: #ffffff;
}
.reviewing-container{
  display: flex;
  flex-direction: row;
  justify-content:space-around;
  align-items:center;
  border-bottom: 1rpx solid #c9bfbfa4;
  padding: 30rpx 0;
}
.scoll-h{
  height: 100%;
}
.refresh{
  display: flex;
  flex-direction: row;
  align-items: center;
  align-self: center;
  justify-content: center;
  padding: 10rpx 0;
  border-bottom: 1rpx solid #c9bfbfa4;
}
.section{
  height: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
  align-items: center;
  justify-content: flex-start;
  padding: 0;
  box-sizing: border-box;

}
.pick-tit{
  height: 90rpx;
  line-height: 90rpx;
}
.picker{
  border: 1px solid #999999;
  border-radius: 10rpx;
  padding-left: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  width: 30%;
}
.t-container{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: center;
  justify-content: flex-start;
  box-sizing: border-box;
  border-top: 1rpx solid #dadada;
  border-left: 1rpx solid #dadada;
}
.item{
  width: 233rpx;
  height: 100rpx;
  line-height: 50rpx; 
  text-align: center;
  border-bottom: 1rpx solid #dadada;
  border-right: 1rpx solid #dadada;
  padding: 10rpx 0;
}
.pad-25{
  padding: 0 25rpx 50rpx 25rpx;
}