.account-input{
  padding-left: 30rpx;
  width: 94%;
  margin: 30rpx auto;
  border: 1px solid #999999;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 20rpx;
  font-size: 30rpx;
}
.section{
  height: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
  align-items: center;
  justify-content: flex-start;
  padding: 0;
  box-sizing: border-box;

}
.pick-tit{
  height: 90rpx;
  line-height: 90rpx;
}
.picker{
  border: 1px solid #999999;
  border-radius: 20rpx;
  padding-left: 30rpx;
  height: 90rpx;
  line-height: 90rpx;
  width: 40%;
}
.account-btn{
  width: 100%;
  margin: 50rpx auto;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
}